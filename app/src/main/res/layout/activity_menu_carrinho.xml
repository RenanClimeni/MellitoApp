        android:text="0"
        android:textColor="@color/colorMarrom"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/txtSache"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtPote300"
